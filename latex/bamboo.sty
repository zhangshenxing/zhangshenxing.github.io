% @ means notitle, * means indent
\NewDocumentEnvironment{main}{m O{} o}{%
	\begin{tcolorbox}[mainstyle,title={\translate{#1}\IfNoValueTF{#3}{}{~(#3)}},#2]%
}{\end{tcolorbox}}
\NewDocumentEnvironment{main*}{m O{} o}{%
	\begin{tcolorbox}[mainstyle,title={\translate{#1}\IfNoValueTF{#3}{}{~(#3)}},#2,indent]%
}{\end{tcolorbox}}

